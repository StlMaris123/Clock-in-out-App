<p id="notice"><%= notice %></p>

<h1>Teachers</h1>

<table>
  <thead>
    <tr>
      <th>First Name |</th> 
      <th>Last Name | </th>  
      <th>Phone Number</th> 

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <td><%= teacher.first_name %></td>
        <td><%= teacher.last_name %></td>
        <td><%= teacher.phone_number%></td>      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

